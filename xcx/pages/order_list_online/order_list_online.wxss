 
page {
  background-color: #f6f6f6;
}

.tab_box {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 112rpx;
  padding: 0 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  z-index: 999;
}

.tab_list, .tab_list_cur {
  width: 320rpx;
  height: 72rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #666;
  font-size: 32rpx;
}

.tab_list_cur {
  border-radius: 72rpx;
  color: #333;
  font-weight: bold;
  background-color: #f6f6f6;
}

.order_wrapper {
  position: relative;
  width: 100%;
  padding: 20rpx 24rpx;
  margin-top: 112rpx;
}

.order_list_tip {
  position: fixed;
  top: 112rpx;
  left: 0;
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #698dff;
  background-color: #f0f4ff;
  z-index: 999;
}
.ico_warning {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 32rpx;
  height: 32rpx;
  border: 1px solid #698dff;
  border-radius: 50%;
  font-size: 24rpx;
  font-weight: bold;
  color: #698dff;
  margin-right: 12rpx;
}
